{
  "name": "silverback",
  "version": "0.0.5",
  "description": "The Silverback Engine is typescript entity game framework.",
  "main": "silverback.js",
  "scripts": {
    "build": "webpack --progress --profile --colors --display-error-details --display-cached",
    "build:prod": "webpack --progress --profile --colors --display-error-details --display-cached --optimize-occurence-order --optimize-minimize --optimize-dedupe",
    "test": "karma start karma.conf.js --single-run",
    "test:watch": "karma start karma.conf.js",
    "docs": "typedoc --options typedoc.json src/**/*.ts",
    "postinstall": "tsd install",
    "ts": "tsc",
    "lint": "tslint -c tslint.json src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "repo.gorillabyte.com:cos/silverback.git"
  },
  "engines": {
    "node": ">= 4.2.1 <= 5",
    "npm": ">= 3"
  },
  "author": "Stefan Herndlbauer <sherndlbauer@gorillabyte.com>",
  "license": "MIT",
  "keywords": [
    "typescript",
    "game",
    "entity",
    "silverback",
    "framework",
    "engine"
  ],
  "devDependencies": {
    "chai": "^3.4.1",
    "del": "^2.1.0",
    "istanbul": "^1.0.0-alpha.2",
    "istanbul-instrumenter-loader": "^0.1.3",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.15",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^0.5.3",
    "karma-jasmine": "^0.3.6",
    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^1.1.3",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-webpack": "^1.7.0",
    "matcha": "^0.6.0",
    "mocha": "^2.3.4",
    "phantomjs": "^1.9.19",
    "ts-loader": "^0.7.2",
    "tsd": "^0.6.5",
    "tslint": "^3.2.1",
    "typedoc": "^0.3.12",
    "typescript": "^1.7.5",
    "vinyl-source-stream": "^1.1.0",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "mini-signals": "^1.1.0"
  }
}
