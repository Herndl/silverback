{
  "name": "silverback-engine",
  "version": "1.1.0",
  "description": "Silverback is a game framework for making games and interactive apps.",
  "scripts": {
    "build": "webpack --progress --profile --colors --display-error-details --display-cached",
    "build:prod": "webpack --progress --profile --colors --display-error-details --display-cached --config webpack.config.prod.js",
    "dev": "webpack --progress --profile --colors --display-error-details --display-cached --watch",
    "test": "./node_modules/mocha/bin/mocha 'src/**/*.spec.{ts,tsx}' --require ts-node/register --require jsdom-global/register",
    "karma": "karma start --single-run",
    "coverage": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "prettier": "find ./src/**/*.ts | xargs ./node_modules/prettier/bin-prettier.js --write --parser typescript --single-quote true --print-width 120 --bracket-spacing true --trailing-comma es5 --tab-width 4",
    "bench": "tsc && node ./js/core/bench/index.js",
    "docs": "typedoc --json typedoc.json --out docs src/**/*.ts --mode modules --target ES6 --tsconfig ./tsconfig.json",
    "lint": "tslint -c tslint.json src/**/*.ts",
    "examples": "node tools/server.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gorillabyte/silverback.git"
  },
  "engines": {
    "node": ">= 8.5.0"
  },
  "author": "Stefan Herndlbauer <sherndlbauer@gorillabyte.com>",
  "license": "Apache-2.0",
  "main": "./dist/silverback.js",
  "types": "./dist/silverback.d.ts",
  "keywords": [
    "typescript",
    "game",
    "entity",
    "typescript",
    "server",
    "nodejs",
    "game-server",
    "silverback",
    "framework",
    "engine"
  ],
  "devDependencies": {
    "@types/benchmark": "^1.0.31",
    "@types/chai": "^4.1.0",
    "@types/express": "^4.11.0",
    "@types/karma": "^1.7.3",
    "@types/mocha": "^2.2.46",
    "@types/node": "^9.3.0",
    "@types/pixi.js": "^4.6.3",
    "babel-core": "^6.10.4",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "benchmark": "^2.1.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "express": "^4.16.2",
    "ghooks": "2.0.2",
    "hoek": "~> 5.0.3",
    "https-proxy-agent": "^2.2.0",
    "istanbul": "^1.1.0-alpha.1",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jsdom": "11.5.1",
    "jsdom-global": "3.0.2",
    "karma": "^3.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-coverage-istanbul-reporter": "^1.3.3",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^3.0.0",
    "mini-signals": "^1.2.0",
    "mixin-deep": "^1.3.1",
    "mocha": "^4.1.0",
    "preact": "^8.2.7",
    "prettier": "^1.10.2",
    "ts-loader": "^3.2.0",
    "ts-node": "^4.1.0",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.6.0",
    "typedoc": "^0.9.0",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.10.1"
  },
  "dependencies": {}
}
